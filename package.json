{
  "name": "booksearchengine",
  "version": "1.0.0",
  "description": "This is an app designed to search for books via Google API",
  "main": "server/server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "cd server && npm start",
    "start:dev": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "seed": "cd server && npm run seed",
    "heroku-postbuild": "cd client && npm run build"
  },
  "keywords": [],
  "author": "Francisco Puma",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^5.3.0"
  },
  "dependencies": {
    "@apollo/react-hooks": "^4.0.0",
    "apollo-boost": "^0.4.9",
    "apollo-server-express": "^3.3.0",
    "bcrypt": "^5.0.1",
    "express": "^4.17.1",
    "faker": "^5.5.3",
    "graphql": "^15.5.2",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "nodemon": "^2.0.12",
    "react-router-dom": "^5.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PanchoPuma/Book-Search-Engine.git"
  },
  "bugs": {
    "url": "https://github.com/PanchoPuma/Book-Search-Engine/issues"
  },
  "homepage": "https://github.com/PanchoPuma/Book-Search-Engine#readme"
}
